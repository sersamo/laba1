#include <iostream>
#include <time.h>


using namespace std;

int main(){
	srand(time(NULL));
	int n;
	cin >> n;
	double** array = new double*[n];
	for (int i = 0; i < n; i++){
		array[i] = new double[n];
	}
	for (int i = 0; i < n; i++){
		for (int j = 0; j < n; j++){
			array[i][j] = rand() % 15; 
		}
	}








	for (int i = 0; i < n;i++)
		delete [] array[i];
	delete [] array;
	

	return 0;
}
